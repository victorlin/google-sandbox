{
  "name": "gmail-electron",
  "productName": "gmail-electron",
  "version": "0.1.0",
  "main": "build/App.js",
  "scripts": {
    "compile": "tsc",
    "start": "tsc && electron-forge start",
    "package": "tsc && electron-forge package",
    "make": "tsc && electron-forge make"
  },
  "author": "Victor Lin",
  "license": "Apache-2.0",
  "devDependencies": {
    "@electron-forge/cli": "^7.6.0",
    "@electron-forge/maker-dmg": "^7.6.0",
    "electron": "^33.2.1",
    "typescript": "^4.9.5"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "name": "Gmail",
            "icon": "icons/icon.png"
          }
        }
      ]
    }
  }
}
